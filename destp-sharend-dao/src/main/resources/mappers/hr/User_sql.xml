<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="User">
    <insert id="createUser" keyProperty="id" parameterType="user">
        INSERT INTO user (username,password,phone,email) VALUES (#username#,#password#,#phone#,#email#);
    </insert>
    <select id="selectUser" parameterType="int"
            resultType="mybatis.demo.UserInfo">
        select * from UserInfo where userid =#{userid}
    </select>
</mapper>





