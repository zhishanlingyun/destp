<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<typeAlias alias="user" type="destp.base.biz.hr.dto.User"/>
<mapper namespace="User">
    <insert id="createUser" keyProperty="id" parameterType="user">
        INSERT INTO user (username,password,phone,email) VALUES (#username#,#password#,#phone#,#email#);
    </insert>
</mapper>


